<%- include('./partials/header') -%>

    <h1>video player</h1>
    <div class="container">
        <div class="row">

            <% d.forEach((v)=>{  %>
            <div class="col-md-4 col-sm-6">
                <h2><%= v.title %></h2>
                <video width='320' height='240' controls>
                    <source src=<%= v.address%> type='video/mp4'>

                </video>
            </div>

            <% }) %>

        </div>

    </div>
<%- include('./partials/footer') -%>